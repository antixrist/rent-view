<div class="block-search-controls">

    <div class="order">

        <label for="order-date">Сортировка:</label>

        <input id="order-date" type="radio" name="order" {{? it.order === 'date' }} checked="checked" {{?}}
        value="date">
        <label for="order-date">по дате добавления</label>

        <input id="order-price" type="radio" name="order" {{? it.order === 'price' }} checked="checked" {{?}}
        value="price">
        <label for="order-price">по цене</label>

    </div>

</div>

<div class="block-search-results">

    <div class="search-results-head">
        <h1>Снять {{? it.realty == 'flat' }}квартиру{{??}}комнату{{?}} в Санкт-Петербурге</h1>
    </div>

    <div class="result-count">
        Результатов: {{= it.count_items }}
    </div>

    <div class="search-results-items">

        {{~ it.array :item:index }}

        <a href="/page" class="search-results-item" target="_blank">

            <div class="column-left">
                <div class="hover-img">
                    {{? item.photos.low.length === 0 }}

                    <span class="column" style="width: 100%;left: 0%;"></span>
                    <img src="/img/blue.png" onerror="imgOnError(this)">

                    {{?? item.photos.low.length === 1 }}

                    <span class="column" style="width: 100%;left: 0%;"></span>
                    <img src="{{? typeof item.photos.low[0] !== 'undefined' }} {{= item.photos.low[0] }} {{??}} /img/blue.png {{?}}"
                         onerror="imgOnError(this)">

                    {{?? item.photos.low.length === 2 }}

                    <span class="column" style="width: 50%;left: 0%;"></span>
                    <img src="{{? typeof item.photos.low[0] !== 'undefined' }} {{= item.photos.low[0] }} {{??}} /img/blue.png {{?}}"
                         onerror="imgOnError(this)">
                    <span class="column" style="width: 50%;left: 50%;"></span>
                    <img src="/img/pic.gif" onerror="imgOnError(this)"
                         data-src="{{? typeof item.photos.low[1] !== 'undefined' }} {{= item.photos.low[1] }} {{??}} /img/blue.png {{?}}">

                    {{?? item.photos.low.length === 3 }}

                    <span class="column" style="width: 33.3%;left: 0%;"></span>
                    <img src="{{? typeof item.photos.low[0] !== 'undefined' }} {{= item.photos.low[0] }} {{??}} /img/blue.png {{?}}"
                         onerror="imgOnError(this)">
                    <span class="column" style="width: 33.3%;left: 33.3%;"></span>
                    <img src="/img/pic.gif" onerror="imgOnError(this)"
                         data-src="{{? typeof item.photos.low[1] !== 'undefined' }} {{= item.photos.low[1] }} {{??}} /img/blue.png {{?}}">
                    <span class="column" style="width: 33.3%;left: 66.6%;"></span>
                    <img src="/img/pic.gif" onerror="imgOnError(this)"
                         data-src="{{? typeof item.photos.low[2] !== 'undefined' }} {{= item.photos.low[2] }} {{??}} /img/blue.png {{?}}">

                    {{?? item.photos.low.length === 4 }}

                    <span class="column" style="width: 25%;left: 0%;"></span>
                    <img src="{{? typeof item.photos.low[0] !== 'undefined' }} {{= item.photos.low[0] }} {{??}} /img/blue.png {{?}}"
                         onerror="imgOnError(this)">
                    <span class="column" style="width: 25%;left: 25%;"></span>
                    <img src="/img/pic.gif" onerror="imgOnError(this)"
                         data-src="{{? typeof item.photos.low[1] !== 'undefined' }} {{= item.photos.low[1] }} {{??}} /img/blue.png {{?}}">
                    <span class="column" style="width: 25%;left: 50%;"></span>
                    <img src="/img/pic.gif" onerror="imgOnError(this)"
                         data-src="{{? typeof item.photos.low[2] !== 'undefined' }} {{= item.photos.low[2] }} {{??}} /img/blue.png {{?}}">
                    <span class="column" style="width: 25%;left: 75%;"></span>
                    <img src="/img/pic.gif" onerror="imgOnError(this)"
                         data-src="{{? typeof item.photos.low[3] !== 'undefined' }} {{= item.photos.low[3] }} {{??}} /img/blue.png {{?}}">

                    {{?? item.photos.low.length >= 5 }}

                    <span class="column" style="width: 20%;left: 0%;"></span>
                    <img src="{{? typeof item.photos.low[0] !== 'undefined' }} {{= item.photos.low[0] }} {{??}} /img/blue.png {{?}}"
                         onerror="imgOnError(this)">
                    <span class="column" style="width: 20%;left: 20%;"></span>
                    <img src="/img/pic.gif" onerror="imgOnError(this)"
                         data-src="{{? typeof item.photos.low[1] !== 'undefined' }} {{= item.photos.low[1] }} {{??}} /img/blue.png {{?}}">
                    <span class="column" style="width: 20%;left: 40%;"></span>
                    <img src="/img/pic.gif" onerror="imgOnError(this)"
                         data-src="{{? typeof item.photos.low[2] !== 'undefined' }} {{= item.photos.low[2] }} {{??}} /img/blue.png {{?}}">
                    <span class="column" style="width: 20%;left: 60%;"></span>
                    <img src="/img/pic.gif" onerror="imgOnError(this)"
                         data-src="{{? typeof item.photos.low[3] !== 'undefined' }} {{= item.photos.low[3] }} {{??}} /img/blue.png {{?}}">
                    <span class="column" style="width: 20%;left: 80%;"></span>
                    <img src="/img/pic.gif" onerror="imgOnError(this)"
                         data-src="{{? typeof item.photos.low[4] !== 'undefined' }} {{= item.photos.low[4] }} {{??}} /img/blue.png {{?}}">

                    {{?}}

                </div>
            </div>

            <div class="column-center">
                <h3 class="head"
                    title="Комната, 6 м² в Санкт-Петербурге">
                    Комната, 6 м²
                </h3>

                <div class="subways">

                    {{~ item.subways :subway_id:subway_index }}

                    <div class="subway">

                        {{? typeof it.subways[subway_id] !== 'undefined' }}

                        {{~ it.subways[subway_id].color :color:color_index }}

                        <span class="subway-label" style="background-color: {{= color }}"></span>

                        {{~}}

                        <span class="subway-name">{{= it.subways[subway_id].name }}</span>

                        {{?}}

                    </div>

                    {{~}}

                </div>

                <div class="source">

                    {{? item.source === 'vk.com' }}
                    <img src="/img/logo/vk.png">
                    <span>vk.com</span>
                    {{?}}

                </div>
            </div>

            <div class="column-right">
                <div class="price">
                    {{? null !== item.price }}
                    {{= item.price.split( /(?=(?:\d{3})+$)/ ).join( ' ' ) }}
                    <span>&#8381; / мес</span>
                    {{??}}
                    Без цены
                    {{?}}
                </div>
                <div class="date">
                    {{= item.timestamp }}
                </div>
            </div>

        </a>

        {{~}}
    </div>

</div>

<div class="block-pagination"></div>

<div class="block-footer"></div>
</body>